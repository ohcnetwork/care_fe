    "react-player": "^2.13.0",
    "react-qr-reader": "^2.2.1",
    "react-redux": "^8.1.1",
    "react-transition-group": "^4.4.5",
    "react-webcam": "^7.1.1",
    "read-excel-file": "^5.6.1",
